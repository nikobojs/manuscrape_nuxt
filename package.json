{
  "name": "nuxt-app",
  "private": true,
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "start": "node .output/server/index.mjs",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "docker-up": "docker-compose up --detach",
    "compile-prisma-postgres": "prisma generate --schema=./prisma/postgres/schema.prisma",
    "compile-prisma-mssql": "prisma generate --schema=./prisma/mssql/schema.prisma",
    "compile-prisma": "yarn compile-prisma-postgres; yarn compile-prisma-mssql",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "migrate-postgres": "npx prisma migrate dev --schema=prisma/postgres/schema.prisma",
    "migrate-mssql": "npx prisma migrate dev --schema=prisma/mssql/schema.prisma",
    "test-dev": "vitest dev",
    "test": "vitest run",
    "full-test-mssql": "./tests/run_all_mssql.sh",
    "full-test-postgres": "./tests/run_all_postgres.sh",
    "full-test": "yarn full-test-mssql; yarn full-test-postgres",
    "test-ui": "vitest --ui"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.60",
    "@iconify-json/heroicons": "^1.2.0",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.13.1",
    "@prisma/client": "^5.16.0",
    "@sentry/node": "^8.17.0",
    "@sentry/tracing": "^7.114.0",
    "@sentry/vue": "^8.17.0",
    "@types/archiver": "^6.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.5.4",
    "@vitest/ui": "^2.0.2",
    "jsdom": "^25.0.0",
    "nuxt": "^3.12.3",
    "nuxt-scheduler": "^0.1.9",
    "pm2": "^5.4.2",
    "prisma": "^5.16.2",
    "sass": "^1.77.7",
    "typescript": "^5.5.3",
    "vuedraggable": "^4.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "@aws-sdk/lib-storage": "^3.616.0",
    "@nuxt/ui": "^2.18.4",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.6.0",
    "exceljs": "^4.4.0",
    "formidable": "^3.5.1",
    "jsonwebtoken": "^9.0.2",
    "stream-buffers": "^3.0.3",
    "v-calendar": "^3.1.2",
    "vitest": "^2.0.2",
    "vue-tsc": "^2.0.26",
    "yup": "^1.4.0"
  }
}
